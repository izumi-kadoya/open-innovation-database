<h1>Upload CSV</h1>

<% if flash[:alert] %>
  <div class="alert alert-danger">
    <%= flash[:alert] %>
  </div>
<% end %>

<% if flash[:notice] %>
  <div class="alert alert-success">
    <%= flash[:notice] %>
  </div>
<% end %>

<%= form_for @record, html: { multipart: true } do |f| %>
  <%= f.file_field :csv %>
  <%= f.submit "Upload" %>
<% end %>
